{
  "name": "shopify-liquid-extension",
  "displayName": "Shopify Liquid Extension",
  "description": "Extension that helps with minor tasks and helps in mapping json files and page relations making the SSO job easier",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "myextension-sidebar-view",
          "title": "My Extension",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "myextension-sidebar-view": [
        {
          "type": "webview",
          "id": "myextension-sidebar",
          "name": "MyExtension",
          "icon": "media/icon.svg",
          "contextualTitle": "MyExtension"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc --force -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.84.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "liquidjs": "^10.9.4"
  }
}
